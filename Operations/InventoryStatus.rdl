  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdventureWorks2019</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProductCategory">
            <Value>=Parameters!ProductCategory.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT p.ProductID,
       p.NAME AS ProductName,
       p.ListPrice,
       pin.Quantity,
       p.SafetyStockLevel,
       p.ReorderPoint,
       pl.Name as Location,
       pl.CostRate,
       pl.Availability,
       p.DaysToManufacture,
       pc.NAME AS Category,
       ps.NAME AS SubCategory
FROM   Production.ProductCategory pc
	   INNER JOIN Production.ProductSubcategory ps ON pc.ProductCategoryID = ps.ProductCategoryID
       INNER JOIN Production.Product p ON p.ProductSubcategoryID = ps.ProductSubcategoryID
       LEFT JOIN Production.ProductInventory pin ON p.ProductID = pin.ProductID
	   INNER JOIN Production.Location pl on pin.LocationID=pl.LocationID
WHERE  p.ProductSubcategoryID IN (SELECT ps.ProductSubcategoryID
                                  FROM   Production.ProductSubcategory ps
                                  WHERE  ProductCategoryID = @ProductCategory)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
